package allprograms;

public class loops {

	public static void main(String[] args) {
		// for loop
		for(int i=1; i<=10; i++)
		{
			System.out.println("*");
		} 
		
// while loop
		int i = 0;
		while (i < 5) {
		  System.out.println(i);
		  i++;
		}
	
		// do while loop
		int j = 1;
		do {
			System.out.println(j);
			j++;
		}
			while(j<5);
		
		
	}

}
